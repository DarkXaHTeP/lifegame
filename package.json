    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "http-server": "^0.9.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1",
    "webpack": "^2.2.1"
