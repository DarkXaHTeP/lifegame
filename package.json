{
  "name": "lifegame",
  "version": "1.0.0",
  "description": "Conway's game of life using React.JS",
  "main": "index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "build:prod": "browserify src/index.jsx --extension=.jsx -t babelify | uglifyjs > public/bundle.js",
    "build:dev": "browserify src/index.jsx --extension=.jsx -t babelify -o public/bundle.js",
